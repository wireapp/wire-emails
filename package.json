{
  "name": "wire-emails",
  "version": "1.0.43",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "yarn translate:process && gulp build --production && yarn text && yarn media",
    "deploy": "bin/deploy.sh",
    "media": "node bin/media.js",
    "precommit": "lint-staged",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.{js,scss,md}\"",
    "start": "gulp",
    "test": "yarn build",
    "text": "node bin/generate-text-emails.js",
    "translate": "yarn translate:upload && yarn translate:download",
    "translate:download": "crowdin download && yarn translate:process",
    "translate:process": "node bin/translate.js",
    "translate:upload": "crowdin upload sources"
  },
  "dependencies": {
    "foundation-cli": "2.2.5",
    "foundation-emails": "2.2.1",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "global": "4.3.2",
    "html-to-text": "4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "beepbeep": "1.2.1",
    "browser-sync": "2.23.6",
    "colors": "1.2.1",
    "gulp": "4.0.0",
    "gulp-awspublish": "3.3.3",
    "gulp-cli": "2.0.1",
    "gulp-html-src": "1.0.0",
    "gulp-htmlmin": "4.0.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "4.1.0",
    "gulp-inline-css": "3.3.1",
    "gulp-litmus": "0.0.7",
    "gulp-load-plugins": "1.5.0",
    "gulp-mail": "0.3.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uncss": "1.0.6",
    "gulp-zip": "4.1.0",
    "husky": "0.14.3",
    "inky": "1.3.7",
    "lazypipe": "1.0.1",
    "lint-staged": "7.0.0",
    "merge-stream": "1.0.1",
    "panini": "https://github.com/zurb/panini.git#v2.0",
    "prettier": "1.11.1",
    "replace-in-file": "3.2.0",
    "rimraf": "2.6.2",
    "siphon-media-query": "1.0.0",
    "yargs": "11.0.0"
  },
  "lint-staged": {
    "*.{js,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
