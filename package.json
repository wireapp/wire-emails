{
  "name": "wire-emails",
  "version": "1.0.44",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build": "yarn translate:process && gulp build --production && yarn text && yarn media",
    "deploy": "bin/deploy.sh",
    "media": "node bin/media.js",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.{js,scss,md,yml,yaml}\"",
    "start": "gulp",
    "test": "yarn build",
    "text": "node bin/generate-text-emails.js",
    "translate": "yarn translate:upload && yarn translate:download",
    "translate:download": "crowdin download && yarn translate:process",
    "translate:process": "node bin/translate.js",
    "translate:upload": "crowdin upload sources"
  },
  "dependencies": {
    "foundation-cli": "2.2.5",
    "foundation-emails": "2.2.1",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "global": "4.3.2",
    "html-to-text": "4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "beepbeep": "1.2.2",
    "browser-sync": "2.26.3",
    "colors": "1.3.3",
    "gulp": "4.0.0",
    "gulp-awspublish": "4.0.0",
    "gulp-cli": "2.0.1",
    "gulp-html-src": "1.0.0",
    "gulp-htmlmin": "5.0.1",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "5.0.3",
    "gulp-inline-css": "3.3.1",
    "gulp-litmus": "0.0.7",
    "gulp-load-plugins": "1.5.0",
    "gulp-mail": "0.3.0",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uncss": "1.0.6",
    "gulp-zip": "4.2.0",
    "husky": "1.3.1",
    "inky": "1.3.7",
    "lazypipe": "1.0.2",
    "lint-staged": "8.1.0",
    "merge-stream": "1.0.1",
    "panini": "https://github.com/zurb/panini.git#v2.0",
    "prettier": "1.16.1",
    "replace-in-file": "3.4.3",
    "rimraf": "2.6.3",
    "siphon-media-query": "1.0.0",
    "yargs": "12.0.5"
  },
  "lint-staged": {
    "*.{js,scss,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
